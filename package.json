{
  "name": "static-site-boilerplate",
  "description": "A modern boilerplate for static website development",
  "version": "1.0.0",
  "homepage": "http://staticsiteboilerplate.com",
  "author": "Eric Alli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericalli/static-site-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericalli/static-site-boilerplate/issues"
  },
  "keywords": [
    "html5",
    "es6",
    "frontend",
    "sass",
    "static",
    "boilerplate",
    "webpack"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js",
    "setup": "node ./setup.js",
    "postinstall": "node ./setup.js",
    "lint:js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "build:dist": "webpack --config ./config/webpack.config.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webapp-webpack-plugin": "^2.4.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5",
    "webpackbar": "^3.1.4"
  },
  "dependencies": {
    "enquirer": "^2.2.0",
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.1",
    "reset-css": "^4.0.1",
    "sanitize.css": "^8.0.0",
    "webpack-dev-server": "^3.1.10"
  }
}
